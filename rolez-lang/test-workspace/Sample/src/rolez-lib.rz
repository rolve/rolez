package rolez.lang

mapped class Object {
	mapped def readwrite equals(val other: readwrite Object): boolean
	mapped def readonly hashCode: int
	mapped def readonly toString: pure String
}

mapped class String {
	mapped new(val chars: pure Array[char])
	
	mapped def pure length: int
	mapped def pure trim: pure String
	mapped def pure charAt(val position: int): char
	
	mapped override pure equals(val other: readwrite Object): boolean
	mapped override pure hashCode: int
}

mapped class Array[T] {
	mapped new(val i: int)
	mapped val length: int
	mapped def readonly  get(val i: int): T
	mapped def readwrite set(val i: int, val o: T):
}