package foo

import rolez.util.Scanner

main task Main(args: readonly Array[pure String]): {
	val scanner = new Scanner(the System.in);
	
	var continue = true;
	while(continue) {
		val line = scanner.nextLine;
		if(line.equals("stop"))
			continue = false;
		else
			the System.out.println("Hello " + line + "!");
	}
	
	for(var i = 0; i < 10; i += 1)
		the System.out.println("Good bye!".length);
}
