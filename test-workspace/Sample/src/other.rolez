package foo

class SomeImportantClass {
	var foo: readwrite Account
	var i: int
	val j: int
	
	new {
		this.j = 0;
	}
	
	def pure bar: readonly Account {
		if(true)
			return new Account;
		return null;
	}
	
	def readonly bar(a: int): {
		this.bar;
	}
	
	def atomic baz(foo: atomic Object): {
		val t = this;
		t.bar;
		t.baz(new SomeImportantClass);
		
		atomic (val t1 = this, val t2 = t) {
			t2.i = 0;
		}
	}
}
